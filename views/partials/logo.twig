{% if site.url %}
	<a class="flex items-center space-x-3 rtl:space-x-reverse" href="{{ site.url }}">

<img src="{{ custom_logo_url }}" alt="{{ site.name }}" class="h-8">

<span class="self-center text-2xl font-semibold whitespace-nowrap dark:text-white" >{{ site.name }} </span>

	</a>

{% else %}
	<a href="{{ site.url }}" class="flex items-center">
		<h1 class="text-2xl font-bold text-gray-800">{{ site.title }}</h1>
	</a>
{% endif %}
